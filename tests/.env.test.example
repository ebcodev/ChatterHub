# API Provider Test Configuration
# Copy this file to .env.test and add your API keys

# API Keys (required for testing each provider)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GEMINI_API_KEY=...
XAI_API_KEY=xai-...
DEEPSEEK_API_KEY=...
OPENROUTER_API_KEY=sk-or-...

# Test Configuration
# Which providers to test (set to true to enable)
TEST_OPENAI=true
TEST_ANTHROPIC=true
TEST_GEMINI=true
TEST_XAI=false         # Expensive, disabled by default
TEST_DEEPSEEK=false     # Optional
TEST_OPENROUTER=false   # Optional

# Test Settings
TEST_TIMEOUT=30000      # Timeout in milliseconds
TEST_QUICK_MODE=false   # Only test one model per provider
TEST_VERBOSE=false      # Show response samples
TEST_MAX_COST=1.00      # Maximum cost in USD before stopping tests